a=20
b=20
if [ $a -eq $b ]
then 
  echo "a等于b"
elif [ $a -gt $b ]
then 
  echo "a 大于b"
elif [ $a -lt $b ] 
then 
  echo "a 小于 b"
else
  echo "没有符合的条件"
fi


for i in 1 2 3 4 5
  do 
    echo "for loop: $i"
  done

for i in this is a str
  do 
    echo "for loop:$i"
  done

i=1 
while (($i <= 5))
  do 
    echo "while loop: $i"
    let i++
 done
 


j=2
until [ $j -gt 5 ]
do
  echo "util loop: $j "
  let j++
done

